package 練習;

import javax.swing.JOptionPane;

public class 測試 {
public static void main(String[] args) {
	String sb = JOptionPane.showInputDialog(567);
	for (int i = 0; i < 100; i++) {
		if (sb.equals("123" )) {
			JOptionPane.showInputDialog(null);
		}else {
			System.out.println("你來亂的");
		}
	}
	System.out.println(sb);
}
}
